GMAT acc_magnitude.FixedWidth = true;
GMAT acc_magnitude.Delimiter = ' ';
GMAT acc_magnitude.ColumnWidth = 23;
GMAT acc_magnitude.WriteReport = true;


%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;
Propagate j2_drag_prop(HD_mode) j2_drag_prop(LD_mode) {HD_mode.ElapsedSecs = 12000.0};
Report 'acc' HD_components HD_mode.j2_drag_prop_ForceModel.Acceleration LD_mode.j2_drag_prop_ForceModel.Acceleration;
